<%= simple_form_for(@conteudo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :titulo %>
    <%= f.input :link %>
    <%= f.input :texto %>
    <%= f.input :ativo %>
    <%= f.input :tipo %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
